{
    "name": "Reload All",
    "description": "Reloads all tabs, across all windows",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "128": "icons/128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/128.png"
        }
    }
}
